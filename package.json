{
  "name": "@ng-apimock/dev-interface",
  "description": "Protractor plugin for ng-apimock",
  "version": "1.0.1",
  "homepage": "https://github.com/ng-apimock/dev-interface#readme",
  "author": {
    "name": "Mischa Dasberg",
    "email": "mischa@dasberg.nl"
  },
  "main": "dist/index.html",
  "scripts": {
    "ng": "ng",
    "ngapimock": "node server.js &",
    "start": "npm run ngapimock && ng serve --proxy-config proxy.config.json --host=0.0.0.0",
    "prebuild": "rm -rf dist",
    "build": "ng build --aot --prod",
    "lint": "ng lint",
    "test": "jasmine-ts \"src/**/*.spec.ts\" --project=src/tsconfig.spec.json"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/ng-apimock/dev-interface.git"
  },
  "bugs": {
    "url": "https://github.com/ng-apimock/dev-interface/issues"
  },
  "licenses": [
    {
      "type": "MIT",
      "url": "https://github.com/ng-apimock/dev-interface/blob/master/LICENSE-MIT"
    }
  ],
  "keywords": [
    "apimock",
    "ng-apimock",
    "http",
    "api",
    "local development",
    "scenario",
    "mocking",
    "endpoints",
    "responses",
    "delay",
    "recording"
  ],
  "devDependencies": {
    "@angular-devkit/build-angular": "0.7.5",
    "@angular/cli": "6.1.5",
    "@angular/compiler-cli": "6.1.7",
    "@angular/language-service": "6.1.7",
    "@ng-apimock/core": "1.0.2",
    "@ng-apimock/test-application": "1.0.4",
    "@types/jasmine": "2.8.8",
    "@types/node": "10.9.4",
    "@types/sinon": "5.0.2",
    "codelyzer": "4.4.4",
    "connect": "3.6.6",
    "jasmine-ts": "0.2.1",
    "sinon": "6.2.0",
    "ts-node": "7.0.1",
    "tslint": "5.11.0",
    "typescript": "2.9.2"
  },
  "dependencies": {
    "@angular/animations": "6.1.7",
    "@angular/common": "6.1.7",
    "@angular/compiler": "6.1.7",
    "@angular/core": "6.1.7",
    "@angular/forms": "6.1.7",
    "@angular/http": "6.1.7",
    "@angular/platform-browser": "6.1.7",
    "@angular/platform-browser-dynamic": "6.1.7",
    "@angular/router": "6.1.7",
    "angular2-prettyjson": "3.0.1",
